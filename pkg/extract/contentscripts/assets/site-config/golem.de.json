    "table-jtoc",
    "implied",
    "social-tools",
    "comments",
    "footer",
    "job-market",
    "tags",
    "topictags",
    "go-button-bar"
  ],
  "strip_image_src": null,
  "native_ad_selectors": null,
  "tidy": false,
  "prune": false,
  "autodetect_on_failure": true,
  "single_page_link_selectors": null,
  "next_page_link_selectors": [
    "//li[contains(@class, 'go-pagination__item--next')]/a"
  ],
  "replace_strings": [
