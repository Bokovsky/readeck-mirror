{
  "title_selectors": [
    "//h1[@class=\"entry-title\"]"
  ],
  "body_selectors": [
    "//div[contains(concat(' ',normalize-space(@class),' '),' entry-content ')] | //header/div[contains(@class, 'subheadline')] | //div[contains(@class, 'post-thumbnail')]"
  ],
  "date_selectors": [
    "substring-before(substring-after(//script[@type=\"application/ld+json\"], '\"datePublished\":\"'), '\",\"dateModified\"')",
    "//time[contains(concat(' ',normalize-space(@class), ' '), ' entry-date ')]/@datetime"
  ],
  "author_selectors": [
    "//div[contains(@class, 'entry-meta')]//a[contains(@href, '/author/')]",
    "substring-after( //div[contains(@class, 'entry-meta')]//div[contains(@class, 'meta-text-top')] , 'By ')",
    "//div[@class=\"author-name\"]/a[1]"
  ],
  "strip_selectors": [
    "//div[@class='blogLabel']",
    "//h1",
    "//div[@class=\"article-meta\"]",
    "//div[@class=\"author-info\"]",
    "//div[@class=\"department\"]",
    "//div[@class=\"cap-main\"]",
    "//div[@id=\"compare-lede\"]"
  ],
  "strip_id_or_class": null,
  "strip_image_src": null,
  "native_ad_selectors": null,
  "tidy": false,
  "prune": false,
  "autodetect_on_failure": true,
  "single_page_link_selectors": null,
  "next_page_link_selectors": null,
